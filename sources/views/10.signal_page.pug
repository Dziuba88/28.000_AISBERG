extends layouts/baseLayout
block variables
  - var pageTitle = 'SIGNAL PAGE'
block header
  .b__header--content
    .row
      .col.md__6
        h1.page--title DOMinance
          .tooltip
            svg.tooltip__icon: use(xlink:href="#info-icon")
            .tooltip__menu
              .tooltip__menu--title h4 size 17px/20px
              .tooltip__menu--text p - From troubled dreams, <a href="">he found himself transformed</a> in his bed into a horrible vermin. He lay on his 
        p P - From troubled dreams, he found himself transformed in his bed into a horrible vermin. He lay on his armour-like back, and if he lifted
      .col.md__6: .signals__filters
        .form__row: select
          option(data-display="Currencies") 
          option(value="01" selected) BTC/USDT
          option(value="02") Currencies 2
          option(value="03") Currencies 3
          option(value="04") Currencies 4
        .form__row: select
          option(data-display="Timeframe") 
          option(value="01" selected) M1
          option(value="02") Timeframe 2
          option(value="03") Timeframe 3
          option(value="04") Timeframe 4
        .form__row: select
          option(data-display="Stock") 
          option(value="01" selected) Binance
          option(value="02") Stock 2
          option(value="03") Stock 3
          option(value="04") Stock 4
    .b__header--navs: ul
      li(): a(href="").active All Signals
      li(): a(href="") Popular
      li(): a(href="") Subscribed
