      .panel__close: svg(): use(xlink:href="#close-icon")
      h3.panel__header Get more AIsberg DOMinance prediction signals
      .row
        .col.sm__3.xs__6: .plan
          .plan__title M1
          .plan__info For Free 

        .col.sm__3.xs__6: .plan
          .plan__title M5
          .plan__info 
            strong.text__cta $100/mo
            span &nbsp;or&nbsp;
            strong.text__cta 10 AIS/mo
          a.btn.cta(href="") subscribe
        .col.sm__3.hide__mobile: .plan
          .plan__title M15
          .plan__info 
            strong.text__cta $100/mo
            span &nbsp;or&nbsp;
